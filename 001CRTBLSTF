CREATE TABLE STAFF               
(                                
ID      SMALLINT  NOT NULL ,     
NAME    VARCHAR(9)         ,     
DEPT    SMALLINT           ,     
JOB     CHAR(5)            ,     
"YEARS" SMALLINT           ,     
SALARY  DECIMAL(7,2)       ,     
COMM    DECIMAL(7,2)             
) IN DATABASE TRNGDB2;           
